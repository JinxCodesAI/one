{
  "name": "@scope/ai-api",
  "version": "0.0.1",
  "tasks": {
    "dev": "deno run --allow-net --allow-env --allow-read --watch main.ts",
    "dev:debug": "deno run --allow-net --allow-env --allow-read --inspect main.ts",
    "start": "deno run --allow-net --allow-env --allow-read main.ts",
    "build": "deno compile --allow-net --allow-env --allow-read --output dist/ai-api main.ts",
    "build:linux": "deno compile --allow-net --allow-env --allow-read --target x86_64-unknown-linux-gnu --output dist/ai-api-linux main.ts",
    "test": "deno test --allow-net --allow-env --allow-read --allow-write --allow-run --allow-sys --ignore=e2e/",
    "test:unit": "deno test --allow-net --allow-env --allow-read --allow-write --ignore=e2e/",
    "test:e2e": "deno test --allow-net --allow-env --allow-read --allow-write --allow-run --allow-sys e2e/**/*.e2e.ts",
    "test:watch": "deno test --allow-net --allow-env --allow-read --allow-write --watch --ignore=e2e/",
    "test:coverage": "deno test --allow-net --allow-env --allow-read --allow-write --allow-run --allow-sys --coverage=coverage/ --ignore=e2e/",
    "test:all": "deno test --allow-net --allow-env --allow-read --allow-write --allow-run --allow-sys",
    "lint": "deno lint",
    "lint:fix": "deno lint --fix",
    "format": "deno fmt",
    "format:check": "deno fmt --check",
    "typecheck": "deno check **/*.ts",
    "clean": "rm -rf dist/ coverage/",
    "health": "curl -f http://localhost:8000/health || echo 'Service not running'"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/testing": "jsr:@std/testing@1",
    "@std/dotenv": "jsr:@std/dotenv@0.225",
    "ai": "npm:ai@^4.3.16",
    "@ai-sdk/openai": "npm:@ai-sdk/openai@1.3.22",
    "@ai-sdk/google": "npm:@ai-sdk/google@1.2.19",
    "@openrouter/ai-sdk-provider": "npm:@openrouter/ai-sdk-provider@0.0.5"
  },
  "exports": {
    ".": "./main.ts",
    "./client": "./sdk/client.ts",
    "./types": "./types.ts"
  },
  "compilerOptions": {
    "lib": ["deno.ns", "es2022", "dom"]
  }
}
